<?xml version="1.0"?>
<block>
  <name>Message File Debug</name>
  <key>sandia_utils_message_file_debug</key>
  <category>[Sandia]/Sandia Utilities</category>
  <import>import sandia_utils</import>
  <make>sandia_utils.message_file_debug($filename)</make>

  <param>
    <name>Filename</name>
    <key>filename</key>
    <type>file_save</type>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <doc>
Print messages to a file
  </doc>

</block>
