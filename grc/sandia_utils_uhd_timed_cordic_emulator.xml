<?xml version="1.0"?>
<block>
  <name>UHD Timed CORDIC Emulator</name>
  <key>sandia_utils_uhd_timed_cordic_emulator</key>
  <category>[Sandia]/Sandia Utilities/Debug</category>
  <import>import sandia_utils</import>
  <make>sandia_utils.uhd_timed_cordic_emulator($filename, $loop, $decimation, $samp_rate, $f0, $short, $epoch)</make>

  <param>
    <name>File Type</name>
    <key>short</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Complex</name>
      <key>False</key>
    </option>
    <option>
      <name>Short</name>
      <key>True</key>
    </option>
  </param>
  <param>
    <name>Filename</name>
    <key>filename</key>
    <type>file_open</type>
  </param>
  <param>
    <name>Loop File?</name>
    <key>loop</key>
    <value>True</value>
    <type>enum</type>
    <hide>part</hide>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
  </param>
  <param>
    <name>Decimation</name>
    <key>decimation</key>
    <type>int</type>
  </param>
  <param>
    <name>Sample Rate</name>
    <key>samp_rate</key>
    <type>float</type>
  </param>
  <param>
    <name>Initial Freq</name>
    <key>f0</key>
    <type>float</type>
    <hide>part</hide>
  </param>
  <param>
    <name>Start Time</name>
    <key>epoch</key>
    <value>0</value>
    <type>float</type>
    <hide>part</hide>
  </param>

  <sink>
    <name>command</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>

  <doc>This block allows for a broadband file to be decimated and retuned at specific times for downstream processing.</doc>

</block>
